pipeline{
    agent any
    stages {
        stage ('Building github code') {
            steps {
                echo "github through pipeline"
                sh "hostname -i"
            }
        }
        stage ('otherbuild in git'){
            agent {
                label 'jenkins-slave1'
            }
            steps {
                echo "Building github code in slave"
                sh "hostname -i"
            }
        }
    }
}
